fun main() {
  println("Ola Mundo")
  var nome = "Kauan"
val nascimento = 1975

println(nome)          // Print the value of name
println(nascimento)     // Print the value of birthyear
val myNum = 5             // Int
   
val myDoubleNum = 5.99    // Double
val myLetter = 'D'        // Char
val myBoolean = true      // Boolean
val myText = "Oi"      // String

println (myNum)
println (myDoubleNum)
println (myLetter)
println (myBoolean)
println (myText)
}
